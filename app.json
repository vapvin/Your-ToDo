{
  "expo": {
    "name": "Your To Do List",
    "slug": "your-todo",
    "description": "Your To Do App made with React Native",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#6dd5ed"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vapvin.YourToDo"
    },
    "android": {
      "package": "co.vapvin.your"
    }
  }
}
